<form method="post" action="{{ path('vin_delete', {'IdProduit': vin.IdProduit}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ vin.IdProduit) }}">
    <button class="btn">Delete</button>
</form>