<form method="post" action="{{ path('divers_delete', {'IdProduit': diver.IdProduit}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ diver.IdProduit) }}">
    <button class="btn">Delete</button>
</form>