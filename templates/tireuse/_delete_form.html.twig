<form method="post" action="{{ path('tireuse_delete', {'IdProduit': tireuse.IdProduit}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ tireuse.IdProduit) }}">
    <button class="btn">Delete</button>
</form>