						<td class="tireuse click-tireuse tireuse-12" id="tireuse-12"></td>
					</tr>
				</table>
			</div>
			{% for tireuse in tireuses %}
				<div class="col-md-12 tireuse-{{tireuse.idTireuse}}-description" id="description-tireuse-inactive">
					<span class="titre-description-tireuse"><h3>{{tireuse.biere.NomProduit}}</h3></span>
					<div class="col-md-5 barre-separation"></div>
					<p class="produit-home-description">{{tireuse.biere.descriptionProduit}}</p>
					<span class="col-md-12 ">
							<div class="col-md-10 barre-separation-2">
								<p>{{tireuse.biere.Brasserie.NomBrasserie}}</p>
							</div>
							<div class="col-md-10 barre-separation-2">
								<p class="col-md-6 produit-home-tableau-gauche">{{tireuse.biere.brasserie.paysFabrication.NomPays}}</p>
								<p class="col-md-6">{{tireuse.biere.typeBiere.nomTypeBiere}}</p>
							</div>
							<div class="col-md-10 barre-separation-2">
								<p class="col-md-6 produit-home-tableau-gauche">{{tireuse.biere.degreeBiere}}Â°</p>
								<p class="col-md-6"></p>
							</div>
					</span>
					{% if tireuse.DisponibleHappyHourTireuse == 1 %}
					<span class="col-md-6 dispo-happy-hour"><div class='glyphicon glyphicon-ok-circle'></div>Disponible en happy hour</span>
					{% else %}
						<span class="col-md-6 dispo-happy-hour"><div class='glyphicon glyphicon-remove-circle'></div>Indisponible en happy hour</span>
					{% endif %}
					<span class="col-md-6 note">
					<div class="col-md-3">Note produit :</div>
					<div class="col-md-9">
						{% if tireuse.biere.NoteProduit>0 %}
							{% for i in 1..tireuse.biere.NoteProduit %}
								<div class="glyphicon glyphicon-star note-star"></div>
							{% endfor %}		
							{% if tireuse.biere.NoteProduit<5 %}
								{% for j in 0..5-tireuse.biere.NoteProduit %}
									<div class="glyphicon glyphicon-star-empty note-star note-star-empty"></div>
								{% endfor %}
							{% endif %}
						{% else %}
							{% for j in 0..4 %}
								<div class="glyphicon glyphicon-star-empty note-star note-star-empty"></div>
							{% endfor %}
						{% endif %}
					</div>
					</span>				
					<span class="col-md-6 note col-md-offset-6">
					<div class="col-md-3">Ma Note :</div>
					<div class="col-md-9">
						<div class="glyphicon glyphicon-star note-star"></div>
						<div class="glyphicon glyphicon-star note-star"></div>
						<div class="glyphicon glyphicon-star note-star"></div>
						<div class="glyphicon glyphicon-star note-star"></div>
						<div class="glyphicon glyphicon-star-empty note-star note-star-empty"></div>
					</div>
					</span>
				</div>
			{% endfor %}
		</div>